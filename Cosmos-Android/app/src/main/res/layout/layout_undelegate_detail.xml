<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/space_2"
    app:cardPreventCornerOverlap="true"
    app:cardCornerRadius="@dimen/space_6"
    app:cardElevation="1.5dp"
    app:cardBackgroundColor="@color/colorTransBg"
    app:cardUseCompatPadding="true">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/space_16">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/str_undelegate_status"
                style="@style/font_ss_12_gray1"/>
            <TextView
                android:id="@+id/undelegate_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:gravity="right"
                style="@style/font_ssl_14_white"
                android:text="-"/>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/space_12"
            android:background="@color/colorGray3"/>

        <RelativeLayout
            android:id="@+id/undelegate_detail_layer0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_8">
            <TextView
                android:id="@+id/undelegate_detail_moniker0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxWidth="@dimen/space_120"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="-"
                style="@style/font_ss_12_gray1"/>
            <TextView
                android:id="@+id/undelegate_detail_amount0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ssl_12_white"/>
            <TextView
                android:id="@+id/undelegate_detail_time0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/undelegate_detail_amount0"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ss_11_gray1"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/undelegate_detail_layer1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_8"
            android:visibility="gone">
            <TextView
                android:id="@+id/undelegate_detail_moniker1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxWidth="@dimen/space_120"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="-"
                style="@style/font_ss_12_gray1"/>
            <TextView
                android:id="@+id/undelegate_detail_amount1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ssl_12_white"/>
            <TextView
                android:id="@+id/undelegate_detail_time1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/undelegate_detail_amount1"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ss_11_gray1"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/undelegate_detail_layer2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_8"
            android:visibility="gone">
            <TextView
                android:id="@+id/undelegate_detail_moniker2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxWidth="@dimen/space_120"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="-"
                style="@style/font_ss_12_gray1"/>
            <TextView
                android:id="@+id/undelegate_detail_amount2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ssl_12_white"/>
            <TextView
                android:id="@+id/undelegate_detail_time2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/undelegate_detail_amount2"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ss_11_gray1"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/undelegate_detail_layer3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_8"
            android:visibility="gone">
            <TextView
                android:id="@+id/undelegate_detail_moniker3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxWidth="@dimen/space_120"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="-"
                style="@style/font_ss_12_gray1"/>
            <TextView
                android:id="@+id/undelegate_detail_amount3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ssl_12_white"/>
            <TextView
                android:id="@+id/undelegate_detail_time3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/undelegate_detail_amount3"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ss_11_gray1"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/undelegate_detail_layer4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_8"
            android:visibility="gone">
            <TextView
                android:id="@+id/undelegate_detail_moniker4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxWidth="@dimen/space_120"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="-"
                style="@style/font_ss_12_gray1"/>
            <TextView
                android:id="@+id/undelegate_detail_amount4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ssl_12_white"/>
            <TextView
                android:id="@+id/undelegate_detail_time4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/undelegate_detail_amount4"
                android:layout_alignParentRight="true"
                android:gravity="right"
                android:text="-"
                style="@style/font_ss_11_gray1"/>
        </RelativeLayout>
    </LinearLayout>
</android.support.v7.widget.CardView>